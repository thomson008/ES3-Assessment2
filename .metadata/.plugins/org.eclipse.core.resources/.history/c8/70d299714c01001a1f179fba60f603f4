#include "gpio_init.h"

XStatus initGpio()
{
	XStatus status;

	status = XGpio_Initialize(&VGA_REGION, 10);

	if (status != XST_SUCCESS)
		return XST_FAILURE;

	status = XGpio_Initialize(&VGA_COLOUR, 8);

	if (status != XST_SUCCESS)
		return XST_FAILURE;

	status = XGpio_Initialize(&SWITCHES, 7);

	if (status != XST_SUCCESS)
		return XST_FAILURE;

	return XST_SUCCESS;
}

